for i in range(5,0,-1):
    for j in range(0,5):
        print(i,end="")
    print()

#Iteration :

i               j               print               print
5               1               5
                2               55
                3               555
                4               5555
                5               55555               next line
4               1               4
                2               44
                3               444
                4               4444
                5               44444               next line
3               1               3
                2               33
                3               333
                4               3333
                5               33333               next line
2               1               2
                2               22
                3               222
                4               2222
                5               22222               next line
1               1               1
                2               11
                3               111
                4               1111
                5               11111              next line


#Output

55555
44444
33333
22222
11111
